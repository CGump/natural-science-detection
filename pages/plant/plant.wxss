

.up{
  color: rgb(255, 255, 255);
  font-size: 20px;
  font-family: 微软雅黑;
  width: 200px;
  height: 50px;
  vertical-align: middle;
  text-align: center;
  line-height: 45px;
  border-radius: 25px;
  background-color: rgb(26, 160, 225);
}

.show{
  margin: 30rpx 0 0 5%;
}

.show-img{
  width: 675rpx;
}

.analysis-result{
  display: flex;
  flex-direction: column;
  width: 90%;
  margin: 30rpx 0 0 5%;
}

.analysis-result text{
  font-size: 34rpx;
  margin-bottom: 10rpx;
}

.result {
  font-family: 微软雅黑;
  color: rgb(0, 0, 0);
}